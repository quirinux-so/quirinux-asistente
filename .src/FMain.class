Public Sub nomostrar_Click()

If nomostrar.Value Then 
  Shell "cp /usr/local/share/asistente/asistente.desktop /home/$(whoami)/.config/autostart/"
  Else 
    Shell "rm -f /home/$(whoami)/.config/autostart/asistente.desktop"
Endif

End


